{
  "name": "@durable-streams/proxy",
  "version": "0.1.1",
  "description": "Durable proxy for persistent, resumable AI streaming responses",
  "author": "Durable Stream contributors",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/durable-streams/durable-streams.git",
    "directory": "packages/proxy"
  },
  "bugs": {
    "url": "https://github.com/durable-streams/durable-streams/issues"
  },
  "keywords": [
    "durable-streams",
    "proxy",
    "ai",
    "streaming",
    "resumable",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/server/index.cjs",
  "module": "./dist/server/index.js",
  "types": "./dist/server/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/server/index.d.ts",
        "default": "./dist/server/index.js"
      },
      "require": {
        "types": "./dist/server/index.d.cts",
        "default": "./dist/server/index.cjs"
      }
    },
    "./client": {
      "import": {
        "types": "./dist/client/index.d.ts",
        "default": "./dist/client/index.js"
      },
      "require": {
        "types": "./dist/client/index.d.cts",
        "default": "./dist/client/index.cjs"
      }
    },
    "./transports": {
      "import": {
        "types": "./dist/transports/index.d.ts",
        "default": "./dist/transports/index.js"
      },
      "require": {
        "types": "./dist/transports/index.d.cts",
        "default": "./dist/transports/index.cjs"
      }
    },
    "./conformance-tests": {
      "import": {
        "types": "./dist/conformance-tests/index.d.ts",
        "default": "./dist/conformance-tests/index.js"
      },
      "require": {
        "types": "./dist/conformance-tests/index.d.cts",
        "default": "./dist/conformance-tests/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "durable-streams-proxy-conformance": "./dist/conformance-tests/cli.js"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "test:conformance:local": "vitest run --config vitest.conformance.config.ts conformance-tests/local-conformance.test.ts --no-coverage --reporter=default --passWithNoTests=false",
    "test:conformance": "vitest run --config vitest.conformance.config.ts conformance-tests/test-runner.ts --no-coverage --reporter=default --passWithNoTests=false",
    "test:conformance:watch": "vitest --config vitest.conformance.config.ts conformance-tests/test-runner.ts --no-coverage --reporter=default --passWithNoTests=false",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@durable-streams/client": "workspace:*",
    "vitest": "^4.0.0"
  },
  "devDependencies": {
    "@durable-streams/server": "workspace:*",
    "@types/node": "^22.0.0",
    "tsdown": "^0.9.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "src",
    "conformance-tests",
    "vitest.conformance.config.ts"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
