{
  "name": "@durable-streams/server-conformance-tests",
  "version": "0.1.0",
  "description": "Conformance test suite for Durable Streams server implementations",
  "author": "Durable Stream contributors",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
