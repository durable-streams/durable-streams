{
  "name": "@durable-streams/server-conformance-tests",
  "version": "0.1.4",
  "description": "Conformance test suite for Durable Streams server implementations",
  "author": "Durable Stream contributors",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/durable-streams/durable-streams.git",
    "directory": "packages/server-conformance-tests"
  },
  "bugs": {
    "url": "https://github.com/durable-streams/durable-streams/issues"
  },
  "keywords": [
    "durable-streams",
    "conformance",
    "testing",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "server-conformance-tests": "./dist/cli.js",
    "durable-streams-server-conformance": "./dist/cli.js",
    "durable-streams-server-conformance-dev": "./bin/conformance-dev.mjs"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "bin"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@durable-streams/client": "workspace:*",
    "fast-check": "^4.4.0",
    "vitest": "^4.0.0"
  },
  "devDependencies": {
    "tsdown": "^0.9.0",
    "tsx": "^4.19.2",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
