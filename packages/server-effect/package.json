{
  "name": "@durable-streams/server-effect",
  "version": "0.1.0",
  "description": "Effect-based server implementation for Durable Streams",
  "author": "Durable Stream contributors",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/durable-streams/durable-streams.git",
    "directory": "packages/server-effect"
  },
  "bugs": {
    "url": "https://github.com/durable-streams/durable-streams/issues"
  },
  "keywords": [
    "durable-streams",
    "streaming",
    "server",
    "effect",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "start": "tsx src/main.ts"
  },
  "dependencies": {
    "effect": "^3.19.15",
    "@effect/platform": "^0.94.2",
    "@effect/platform-node": "^0.104.1",
    "@effect/experimental": "^0.58.0"
  },
  "peerDependencies": {
    "pg": "^8.0.0",
    "lmdb": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "pg": {
      "optional": true
    },
    "lmdb": {
      "optional": true
    }
  },
  "devDependencies": {
    "@durable-streams/server-conformance-tests": "workspace:*",
    "@types/node": "^22.0.0",
    "@types/pg": "^8.0.0",
    "tsdown": "^0.9.0",
    "tsx": "^4.19.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
