{
  "name": "@durable-streams/protocol",
  "description": "SDK for building protocols and APIs on top of Durable Streams",
  "version": "0.1.0",
  "author": "Durable Stream contributors",
  "license": "Apache-2.0",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "dependencies": {
    "hono": "^4.7.0"
  },
  "peerDependencies": {
    "@durable-streams/client": "workspace:*"
  },
  "devDependencies": {
    "@durable-streams/server": "workspace:*",
    "@durable-streams/client": "workspace:*",
    "tsdown": "^0.9.0",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  }
}
