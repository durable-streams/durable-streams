{
  "name": "@durable-streams/state",
  "description": "State change event protocol for Durable Streams",
  "version": "0.1.0",
  "author": "Durable Stream contributors",
  "license": "Apache-2.0",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "dependencies": {},
  "devDependencies": {
    "@durable-streams/client": "workspace:*",
    "@durable-streams/server": "workspace:*",
    "@durable-streams/writer": "workspace:*",
    "@standard-schema/spec": "^1.0.0",
    "@tanstack/db": "^0.0.1-alpha.77",
    "tsdown": "^0.9.0"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit"
  }
}
